{
  "cli": {
    "version": ">= 16.19.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": { "buildType": "apk" },
      "credentials": { "source": "local" },
      "ios": { "image": "latest" }
    },
    "preview": {
      "distribution": "internal",
      "android": { "buildType": "apk" },
      "credentials": { "source": "local" }
    },
    "production": {
      "autoIncrement": true,
      "android": { "buildType": "apk" },
      "credentials": { "source": "local" }
    }
  },
  "submit": {
    "production": {}
  }
}
