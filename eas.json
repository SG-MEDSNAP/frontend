{
  "cli": {
    "version": ">= 16.19.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": { "buildType": "app-bundle" },
      "ios": { "image": "latest" }
    },
    "preview": {
      "distribution": "internal",
      "android": { "buildType": "app-bundle" }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "API_BASE_URL": "https://hiedu.site"
      },
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
